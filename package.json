{
  "name": "cjtdevs",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.12.13",
    "nodemailer": "^6.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "npm run-script build && node server.js",
    "start-dev": "concurrently --kill-others-on-fail \"react-scripts start .env.local .env\" \"nodemon -r dotenv/config server.js dotenv_config_path=./.env.local\"",
    "build": "react-scripts build",
    "build-dev": "npm run-script build .env.local && node -r dotenv/config server.js dotenv_config_path=./.env.local\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:5000",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devServer": {
    "compress": true,
    "inline": true,
    "port": 8080,
    "allowedHosts": [
      ".amazonaws.com"
    ]
  },
  "devDependencies": {
    "nodemon": "^2.0.9"
  }
}
